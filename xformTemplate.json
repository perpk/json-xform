{
    "fieldset": [
        {
            "from": "projectInfo.name",
            "to": "projectName"
        },
        {
            "fromEach": {
                "field": "dependencies",
                "to": "libraries",
                "fieldset": [
                    {
                        "from" : "fileName",
                        "to" : "dependency",
                        "fromEach": {
                            "field": "vulnerabilities",
                            "fieldset": [
                                {
                                    "from": "name",
                                    "to": "vulnerabilityName"
                                },
                                {
                                    "from": "severity"
                                },
                                {
                                    "from": "cvssv2.score",
                                    "to": "score"
                                },
                                {
                                    "from": "description"
                                }
                            ]
                        }
                    }
                ]
            }
        }
    ]
}